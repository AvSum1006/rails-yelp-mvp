<h1 class="header text-center p-3">New Review</h1>
<div class="p-3">
<%= simple_form_for [@restaurant, @review], :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :rating, collection: (0..5)%>
  <%= f.input :content, class: "m-2" %>
  <div ><%= f.submit class:"btn btn-primary p-2 m-2 "%></div>
<% end %>
</div>
